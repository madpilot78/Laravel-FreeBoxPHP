{
    "name": "madpilot78/laravel-freebox-php",
    "description": "Laravel FreeBoxPHP package",
    "license": "BSD-3-Clause",
    "type": "library",
    "version": "0.0.1",
    "keywords": [
        "api",
        "freebox",
        "laravel",
        "php"
    ],
    "authors": [
        {
            "name": "Guido Falsi",
            "email": "mad@madpilot.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "madpilot78/freebox-php": "^0.0.2",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "friendsofphp/php-cs-fixer": "^3.54",
        "illuminate/testing": "^11.0",
        "madpilot78/php-cs-fixer-config": "^1.0",
        "orchestra/testbench": "^9.2",
        "phpunit/phpunit": "^11"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "madpilot78\\LaravelFreeBoxPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "madpilot78\\LaravelFreeBoxPHP\\FreeBoxPHPServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs:check": "php-cs-fixer fix --dry-run --diff",
        "cs:fix": "php-cs-fixer fix"
    }
}
